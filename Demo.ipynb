   "source": [
    "from cvrpy.pso import PSO\n",
    "\n",
    "pso = PSO(\n",
    "    lambda vec: 1/sum(vec),\n",
    "    num_iterations=100,\n",
    "    num_particles=100,\n",
    "    num_dimensions=20,\n",
    "    range_values=(0.0, 2.0)\n",
    ")\n",
    "\n",
    "solution = pso.start()\n",
    "\n",
    "print(type(solution))\n",
    "print(solution.position)\n"
   ],
